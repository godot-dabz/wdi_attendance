
<div class = "logout">
  <%= button_to "Logout", '/sessions', method: "delete" %>
  <br /><%= button_to "Back", :back %><br />
</div>

<h1>Add a new Student to <%= Cohort.find(@cohort_id).name %>:</h1>

<% @students["students"].each do |student| %>

  <h3><%= student["data"]["first"] + " " + student["data"]["last"] %></h3>

  <%= button_to "Add", "/students", params: {
      name: student["data"]["first"] + " " + student["data"]["last"],
      email: student["contact"]["email"],
      cohort_id: @cohort_id,
      password: "12345",
      number: student["contact"][""]
    } %>

  <% end %>

